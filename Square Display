#include<iostream>
using namespace std;
int main()
{
    int num; 
    cout<<"Enter a positive integer no greater than 15"<<endl;
    cin>>num; 
    for(int i=0;i<num;i++)
    {
        for(int j=0;j<num;j++)
        {
           cout<<"X";
        }
        cout<<endl;
    }
    return 0;
}