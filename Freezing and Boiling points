if(temp<=32)
{
    cout<<"Freez:" <<endl;
    cout<<"Water"<<endl;
    if(temp<=-38)
    {
        cout<<"Mercury"<<endl;
       if(temp<=-173)
       {
            cout<<"Ethyl alcohol"<<endl;
            if(temp<=-362)
            {
               cout<<"Oxygen"<<endl;
                         
            }
                                             }
      
                
                
     
                                                    }         
                                                                            }
if(temp>=-306)
{
   cout<<"Boil:" <<endl;
   cout<<"Oxygen"<<endl;
   if(temp>=172){
    cout<<"Ethyl alcohol"<<endl;
        if(temp>=212){
        cout<<"Water"<<endl;
             if(temp>=676){
                cout<<"Mercury"<<endl;
             }   
                
                
                      }
                 }
  
   }

