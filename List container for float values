#include<iostream>
#include<list>
using namespace std;
int main()
 {
 	list<float>mylist;
 	  list<float>::iterator obj;
 	mylist.push_front(12.7);
 	mylist.push_front(9.65);
 	mylist.push_front(8.72);
 	mylist.push_front( 4.69);
 	cout<<"Before reversing  "<<endl;
    for(  obj= mylist.begin() ;obj!=mylist.end();obj++)
    cout<<*obj <<" " ;
    cout<<endl;
	cout<<"After reversing  "<<endl;
    mylist.reverse();
    for(  obj= mylist.begin() ;obj!=mylist.end();obj++)
    cout<<*obj <<" " ;
 }